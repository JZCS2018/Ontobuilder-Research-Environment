	<xsd:complexType name="OrderDatesType">
		<xsd:annotation>
			<xsd:documentation>is used to hold a list of the pertinent dates for the purchase
                order dates will include validity dates, <!--code-->RequestedShipDate<!--/code-->,
                <!--code-->CancelByDates<!--/code-->,         among others. These dates act as
                defaults that may be overwritten at the item level or package level in the body of
                the message.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RequestedShipByDate" type="xsd:dateTime" form="qualified"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>is the date items in  the purchase order must be shipped from
                        the         seller.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RequestedDeliverByDate" type="xsd:dateTime" form="qualified"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>is the date items in  the purchase order must be received by
                        the buyer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PromiseDate" type="xsd:dateTime" form="qualified" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>is used mainly for confirming orders when a date was promised
                        by         the supplier over the phone. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ValidityDates" form="qualified" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>contains the dates between which the
                        <!--code-->Order<!--/code--> is effective.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CancelByDate" type="xsd:dateTime" form="qualified" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>is the date after which <!--code-->Order<!--/code--> should
                        be cancelled.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ListOfDateCoded" form="qualified" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>contains all other dates not for the
                        <!--code-->Order<!--/code--> not  explicitly stated as the content of other
                        elements. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation xml:lang="en">             XML Common Business Library 4.0
